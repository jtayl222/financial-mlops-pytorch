{
  "dashboard": {
    "id": null,
    "title": "Financial MLOps A/B Testing",
    "tags": ["mlops", "ab-testing"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "schemaVersion": 16,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Request Count by Model",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(ab_test_requests_total) by (model_name)",
            "legendFormat": "{{model_name}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Model Accuracy",
        "type": "stat",
        "targets": [
          {
            "expr": "ab_test_model_accuracy",
            "legendFormat": "{{model_name}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 70}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Response Time (P95)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ab_test_response_time_seconds_bucket[5m]))",
            "legendFormat": "P95 Response Time",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 0.2}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ab_test_requests_total[5m])",
            "legendFormat": "{{model_name}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "yAxes": [
          {
            "label": "Requests/sec",
            "show": true
          },
          {
            "show": true
          }
        ],
        "xAxis": {
          "show": true
        },
        "lines": true,
        "fill": 1,
        "linewidth": 1,
        "points": false,
        "pointradius": 2,
        "bars": false,
        "stack": false,
        "percentage": false,
        "legend": {
          "avg": false,
          "current": false,
          "max": false,
          "min": false,
          "show": true,
          "total": false,
          "values": false
        },
        "nullPointMode": "null"
      }
    ]
  },
  "overwrite": false
}