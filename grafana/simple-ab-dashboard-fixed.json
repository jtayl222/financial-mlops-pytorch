{
  "id": null,
  "title": "Simple A/B Testing Dashboard",
  "tags": ["ab-testing", "financial", "mlops"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Total Requests by Model",
      "type": "stat",
      "targets": [
        {
          "expr": "sum by (model_name) (ab_test_requests_total)",
          "legendFormat": "{{model_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Model Accuracy",
      "type": "stat", 
      "targets": [
        {
          "expr": "ab_test_model_accuracy",
          "legendFormat": "{{model_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Available Metrics",
      "type": "stat",
      "targets": [
        {
          "expr": "up{job=\"financial_ab_test\"}",
          "legendFormat": "Pushgateway Status",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    }
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "refresh": "5s",
  "schemaVersion": 30,
  "version": 1
}